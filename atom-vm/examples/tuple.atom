.data
    open: DB '('
    close: DB ')'
    comma: DB ', '

.main
    PUSH_CONST 3
    PUSH_CONST 2
    PUSH_CONST 1

    # tuple size
    PUSH_CONST 3
    MAKE_TUPLE 0001

    # print tuple
    PUSH open
    PRINT
    LOAD_CONST 0001
    PRINT_CONST
    PUSH comma
    PRINT
    LOAD_CONST 0002
    PRINT_CONST
    PUSH comma
    PRINT
    LOAD_CONST 0003
    PRINT_CONST
    PUSH close
    PRINT
    HALT
